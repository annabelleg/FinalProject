int[][] data;
int rows, cols;

void Matrix(int r, int c){
    rows = r;
    cols = c;
    data = new int[rows][cols];
}

void draw(){
    background(255);
    fill(255);  
    for (int r = 0; r < rows; r++){
        for (int c = 0; c < cols; c++){
            text(data[r][c], r*10, c*10);
        }
    }
}

